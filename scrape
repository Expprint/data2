    $("#subscribeForm [id='email']").keypress(function() {
        $("#subscribeForm [id='email']").css("border", "1px solid #0ab200");
    });
    $("#subscribeForm").submit(function () { 
        var err = 0;
        var str = $(this).serialize();
        var email = $("#subscribeForm [name='email']").val();
        $("#subscribeForm [id='email']").removeClass("input-error");
        if(email=='')
        {	

            $("#subscribeForm [id='email']").css("border", "1px solid #f70404");
            err = 1;
        } 
        if(!validateEmail(email)) {

            $("#subscribeForm [id='email']").css("border", "1px solid #f70404");
            err = 1;
        } 
        if(!err){
            $("#subscribeFormBtn").attr("disabled", true);
            $.ajax(
            {
                type: "POST",
                url: 'https://imprintnext.com/content/themes/imprint-next/newsletter.php',
                fileElementId:'attachment',
                data: str,
                success: function (msg)
                {
                    if(msg == 'Success') {                            
                            $("#newsletter").show();
                        setTimeout(function () {
                            //when it resumes it takes on this timing
                            $("#newsletter").fadeOut('fast');
                            $("#subscribeFormBtn").attr("disabled", false);                             
                        }, 5000);                       
                    }
                }
            });

        } 
        return false;
    });</script> <script>$(document).ready(function () {
        /****** Sticky Navigation Start ******/
        $(window).scroll(function () {
            if ($(window).scrollTop() >= 26) {
                $('#masthead').addClass('fixed-header');
            } else {
                $('#masthead').removeClass('fixed-header');
            }
        });
        /****** Feature Collapse Start ******/
        if (screen.width <= 600) {
            $('.m-collapsebtn').removeClass('active');
            $('.m-collapsebtn').prepend('<span class="ui-icon">');
            $('.m-collapse').removeClass('show');
            $('.m-collapsebtn').click(function (e) {
                e.preventDefault();
                var $this = $(this);
                if ($this.next().hasClass('show')) {
                    $this.removeClass('active');
                    $this.next().removeClass('show');
                    $this.next().slideUp(350);
                } else {
                    $this.parent().parent().find('.m-collapsebtn').removeClass('active');
                    $this.parent().parent().find('.m-collapse').removeClass('show');
                    $this.parent().parent().find('.m-collapse').slideUp(350);
                    
                    $this.next().slideToggle(350);
                    $this.next().toggleClass('show');
                    $this.addClass('active');
                }
            });
        } else {
            $('.m-collapsebtn').addClass('active');
            $('.m-collapse').addClass('show');
        }

        /****** Scroll Spy Start ******/
        let parent = document.querySelector('.sticky').parentElement;

        while (parent) {
            const hasOverflow = getComputedStyle(parent).overflow;
            if (hasOverflow !== 'visible') {
                console.log(hasOverflow, parent);
            }
            parent = parent.parentElement;
        }
        var lastId,
            topMenu = $(".home-product-list-nav"),
            topMenuHeight = topMenu.outerHeight() + 15,
            /* All list items */
            menuItems = topMenu.find("a"),
            /* Anchors corresponding to menu items */
            scrollItems = menuItems.map(function () {
                var item = $($(this).attr("href"));
                if (item.length) {
                    return item;
                }
            });
        menuItems.click(function (e) {
            var href = $(this).attr("href"),
                offsetTop = href === "#" ? 0 : $(href).offset().top - 140;
            /* offsetTop = href === "#" ? 0 : $(href).offset().top - topMenuHeight + 1; */
            $('html, body').stop().animate({
                scrollTop: offsetTop
            }, 300);
            e.preventDefault();
        });
        /* Bind to scroll */
        $(window).scroll(function () {
            /* Get container scroll position */
            var fromTop = $(this).scrollTop() + topMenuHeight;

            /* Get id of current scroll item */
            var cur = scrollItems.map(function () {
                if ($(this).offset().top < fromTop)
                    return this;
            });
            /* Get the id of the current element */
            cur = cur[cur.length - 1];
            var id = cur && cur.length ? cur[0].id : "";

            if (lastId !== id) {
                lastId = id;
                /* Set/remove active class */
                menuItems
                    .parent().removeClass("current")
                    .end().filter("[href='#" + id + "']").parent().addClass("current");
            }
        });
    });</script>  <script>$(function() {
  $('.see-more, .see-demo, .schedule-demo').click(function() {
    if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
      var target = $(this.hash);
      target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
      if (target.length) {
        $('html,body').animate({
          scrollTop: target.offset().top
        }, 1000);
        return false;
      }
    }
  });
});</script>  <script>(function () {
